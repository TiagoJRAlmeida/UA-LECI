#!/bin/bash

FILE_FOLDER=$(dirname $0)

# rep_list_subjects <session file> [username]
if [ -z "$2" ]; then
    python3 ${FILE_FOLDER}/../client.py -k rep_pub_key.pem -r 127.0.0.1:5000 -c rep_list_subjects "$1"
else
    python3 ${FILE_FOLDER}/../client.py -k rep_pub_key.pem -r 127.0.0.1:5000 -c rep_list_subjects "$1" "$2"
fi